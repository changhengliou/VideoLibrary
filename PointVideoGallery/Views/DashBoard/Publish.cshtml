@section header
{
    <link href="/Static/dist/publish.css" rel="stylesheet" />
}
<div class="panel panel-default">
    <div class="panel-heading"><span class="glyphicon glyphicon-cog"></span> 執行作業
    </div>
    <div class="panel-body">
        <div class="form-inline" role="form">
            <div id="schedule-msg"></div>
            <div class="datepicker-form">
                <label for="schedule-datepicker" class="control-label">發布時間</label>
                <input type="text" class="form-control input-sm" id="schedule-datepicker">
            </div>
            <table id="schedule-table"></table>
            <hr/>
            <button type="button" class="btn btn-sm btn-default"><span class="glyphicon glyphicon-refresh"></span> 發佈</button>
            <button type="button" class="btn btn-sm btn-default"><span class="glyphicon glyphicon-download"></span> 下載</button>
            <button type="button" class="btn btn-sm btn-default"><span class="glyphicon glyphicon-file"></span> 報表</button>
        </div>
    </div>
</div>
@section scripts
{
    <script src="~/Static/dist/publish.js"></script>
}
